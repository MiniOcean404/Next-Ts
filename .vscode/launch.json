{
  // 使用 IntelliSense 了解相关属性。
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev-chrome",
      "request": "launch",
      "type": "dart",
      "args": [
        "run -d chrome", // 运行平台
        "--dart-define",
        "APP_CHANNEL=Flutter",
        "--dart-define",
        "ENV=dev"
      ]
    },
    {
      "name": "staging",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "--dart-define",
        "APP_CHANNEL=Flutter",
        "--dart-define",
        "ENV=staging"
      ]
    },
    {
      "name": "prod",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--dart-define",
        "APP_CHANNEL=Flutter",
        "--dart-define",
        "ENV=prod"
      ]
    }
  ]
}
